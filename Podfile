target 'TestWidget' do
  # Comment the next line if you don't want to use dynamic frameworks
  #use_frameworks!
  
  # Pods for Picsfox
  pod 'SnapKit', '~> 5.0.1'
  pod 'AFNetworking', '4.0.1'
  pod 'SDWebImageWebPCoder', '0.5.0'
end
  
target 'MyWidgetExtension' do
  pod 'AFNetworking', '4.0.1'
end
